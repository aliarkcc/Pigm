                <td>
                    <label asp-for="TpsNo">Tps Numarası</label><br />
                    <input asp-for="TpsNo" readonly="readonly" />
                </td>
            </tr>
            <tr>
                <td>
                    <label asp-for="DocumentNo">Belge Numarası</label><br />
                    <input asp-for="DocumentNo" readonly="readonly" />
                </td>
                <td>
                    <label asp-for="RatificationDate">Kabul Tarihi</label><br />
                    <input asp-for="RatificationDate" readonly="readonly" />
                </td>
                <td align="center">
                    <br /><input type="submit" value="Sil" class="btn btn-danger" />
                    <a class="btn btn-info" asp-action="Index">Vazgeç</a>
                </td>
            </tr>
        </form>
    </table>
</div>