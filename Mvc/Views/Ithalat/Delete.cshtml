@model Entities.Concrete.FollowList
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_GlobalLayout.cshtml";
}
<div>
    <table>
        <thead>
        <th style="font-size:25px">Silmek İstediğinize Emin Misiniz?</th>
        </thead>
        <form asp-controller="Ithalat" asp-action="Delete" method="post">
            <tr>
                <td>
                    <label asp-for="InvoiceNo">Fatura Numarası</label><br />
                    <input asp-for="InvoiceNo" readonly="readonly" />
                </td>
                <td>
                    <label asp-for="Segment">Segment</label><br />
                    <input asp-for="Segment" readonly="readonly" />
                </td>
                <td>
                    <label asp-for="SerialNo">Seri Numarası</label><br />
                    <input asp-for="SerialNo" readonly="readonly" />
                </td>
                <td>
                    <label asp-for="Well">Kuyu</label><br />
                    <input asp-for="Well" readonly="readonly" />
                </td>
                <td>
                    <label asp-for="PigmNo">Pigm Numarası</label><br />
                    <input asp-for="PigmNo" readonly="readonly" />
                </td>
                <td>
                    <label asp-for="PigmNo">Pigm Numarası</label><br />
                    <input asp-for="PigmNo" readonly="readonly" />
                </td>
            </tr>
            <tr>
                <td>
                    <label asp-for="PetitionNo">Dilekçe Numarası</label><br />
                    <input asp-for="PetitionNo" readonly="readonly" />
                </td>
                <td>
                    <label asp-for="Used">Kullanılmış mı</label><br />
                    <input asp-for="Used" readonly="readonly" />
